<div class="my-form">
<%= form_for (@store) do |f| %>
    <div class="form-group">
        <%= f.label :name , "店の名前" %>
        <%= f.text_field :name , class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.submit "追加", class: "btn btn-primary" %>
    </div>
<% end %>
</div>